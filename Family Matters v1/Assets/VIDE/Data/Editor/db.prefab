      outputNodeIndex: -1
      outActionIndex: -1
      showmore: 0
      outRect:
        serializedVersion: 2
        x: 0
        y: 0
        width: 0
        height: 0
      sprites: {fileID: 0}
      audios: {fileID: 0}
      visible: 1
    - text: "Omar: \nLaten we de juf halen.\r"
      extraData: ExtraData
      inputSetIndex: 2
      outputNodeIndex: -1
      outActionIndex: -1
      showmore: 0
      outRect:
        serializedVersion: 2
        x: 0
        y: 0
        width: 0
        height: 0
      sprites: {fileID: 0}
      audios: {fileID: 0}
      visible: 1
    - text: "Juf: \nOhjee, wat hebben we hier?\r"
