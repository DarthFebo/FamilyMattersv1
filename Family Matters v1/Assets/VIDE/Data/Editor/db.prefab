      sprites: {fileID: 0}
      audios: {fileID: 0}
      visible: 1
    - text: "John: \r\nHelemaal niet vies, het komt net uit de was\r\n"
      extraData: ExtraData
      inputSetIndex: 6
      outputNodeIndex: -1
      outActionIndex: -1
      showmore: 0
      outRect:
