      showmore: 0
      outRect:
        serializedVersion: 2
        x: 0
        y: 0
        width: 0
        height: 0
      sprites: {fileID: 0}
      audios: {fileID: 0}
      visible: 1
    - text: "Bram: \nDat doe je toch niet!\r"
