      audios: {fileID: 0}
      visible: 1
    - text: "Yvonne: \r\nDe zweetvlekken zijn erin getrokken en er zitten allemaal
        gaatjes daar onderin\r\n"
      extraData: ExtraData
      inputSetIndex: 6
      outputNodeIndex: -1
      outActionIndex: -1
      showmore: 0
      outRect:
        serializedVersion: 2
        x: 0
