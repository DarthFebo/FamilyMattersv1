      sprites: {fileID: 0}
      audios: {fileID: 0}
      visible: 1
    - text: "Yvonne: \r\nHet ging wel. Mark was een beetje overdonderd, keuzestress
        denk ik, dus ze komt later terug. \r\n\r"
      extraData: ExtraData
      inputSetIndex: 41
      outputNodeIndex: -1
      outActionIndex: -1
      showmore: 0
      outRect:
        serializedVersion: 2
        x: 0
